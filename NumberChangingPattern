import java.util.*;
public class NumberChangingPattern {
    public static void main(String[] args) {
        System.out.println("Hello, World!\n");
        int i,j,n=5,num=1;
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=i;j++)
            {
                
                System.out.print(num+" ");
                num++;
                }
                System.out.println();
            }
            
        }
    }
