class ReverseHalfPattern {
    public static void main(String[] args) {
        System.out.println("Hello, World!\n");
        int i,j,n=5;
        for(i=n;i>=1;i--)
        {
            for(j=1;j<=i;j++)
            {
                
                System.out.print("* ");
            }
                System.out.println();
            }
            
        }
    }
/*
o/p
* * * * * 
* * * * 
* * * 
* * 
* 
*/
