import java.util.*;
public class SquareFillPattern {
    public static void main(String[] args) {
        System.out.println("Hello, World!\n");
        int i,j,n=6;
        for(i=0;i<n;i++)
        {
            for(j=0;j<n;j++)
            {
                
                System.out.print("* ");
                }
                System.out.println();
            }
            
        }
    }

/*
